{
    "name": "nikolaposa/zf-disqus",
    "description": "ZF2 module which facilitates integration of Disqus widgets",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "disqus",
        "zf2",
        "zend",
        "module"
    ],
    "homepage": "https://github.com/nikolaposa/ZfDisqus",
    "authors": [
        {
            "name": "Nikola Po≈°a",
            "email": "posa.nikola@gmail.com",
            "homepage": "http://www.nikolaposa.in.rs"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0.3"
        }
    },
    "require": {
        "php": "^7.0",
        "nikolaposa/disqus-helper": "^2.0",
        "zendframework/zend-view": "^2.3",
        "zendframework/zend-servicemanager": "^2.3 || ^3.0.3"
    },
    "require-dev": {
        "zendframework/zend-modulemanager": "^2.3",
        "zendframework/zend-serializer": "^2.3",
        "zendframework/zend-log": "^2.3",
        "zendframework/zend-i18n": "^2.3",
        "zendframework/zend-test": "^2.3",
        "phpunit/phpunit": "^4.7"
    },
    "autoload": {
        "psr-4": { "ZfDisqus\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "ZfDisqus\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
