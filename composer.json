{
    "name": "nikolaposa/zf-disqus",
    "description": "ZF module which facilitates integration of Disqus widgets",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "disqus",
        "zf",
        "zend",
        "module"
    ],
    "authors": [
        {
            "name": "Nikola Po≈°a",
            "email": "posa.nikola@gmail.com",
            "homepage": "http://www.nikolaposa.in.rs"
        }
    ],
    "require": {
        "php": "^7.0",
        "nikolaposa/disqus-helper": "^2.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-view": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "zendframework/zend-modulemanager": "^2.7",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-i18n": "^2.7"
    },
    "autoload": {
        "psr-4": { "ZfDisqus\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "ZfDisqus\\Tests\\": "tests/" }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "cs-fix": "php-cs-fixer fix --config=.php_cs"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
